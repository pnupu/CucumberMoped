# Testnet Environment Configuration
# Copy this to .env.testnet for testing

# Telegram Bot Token (get from @BotFather)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# Wallet Encryption Key (64-character hex string)
# Generate with: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
WALLET_ENCRYPTION_KEY=your_64_character_hex_encryption_key_here

# Database Path (optional, defaults to ./data/testnet-bot.db)
DATABASE_PATH=./data/testnet-bot.db

# 1inch API Configuration (not required for testnet mock)
ONEINCH_API_URL=https://api.1inch.dev/fusion-plus
ONEINCH_API_KEY=dummy_key_for_testnet

# Node.js Environment
NODE_ENV=testnet 